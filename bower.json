{
  "name": "gssb-menu",
  "version": "0.0.1",
  "homepage": "https://github.com/evantill/gssb-menu",
  "authors": [
    "Eric Vantillard <eric.vantillard@evaxion.fr>"
  ],
  "description": "Gestion des Menus de la cantine pour l'Apel GSSB",
  "main": "?",
  "keywords": [
    "apel",
    "gssb",
    "menu",
    "cantine"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "node_modules",
    "bower_components",
    "out"
  ],
  "dependencies": {
    "jquery": "~2.0.3",
    "bootstrap": "~3.0.3",
    "modernizr": "~2.7.1",
    "respond": "~1.4.1",
    "console-polyfill": "~0.1.0",
    "moment": "~2.4.0",
    "date.format": "~1.2.3",
    "cufon": "*"
  },
  "exportsOverride": {
    "bootstrap": {
      "js": "dist/js/*.min.js",
      "fonts": "dist/fonts/*",
      "css": "dist/css/*.min.css"
    },
    "modernizr": {
      "js": "modernizr.js"
    },
    "respond": {
      "js": "dest/*.js"
    },
    "console-polyfill": {
      "js": "**/*.js"
    },
    "*": {
      "js": "**/*.min.js",
      "css": "**/*.min.css"
    }
  }
}
