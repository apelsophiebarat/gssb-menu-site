{
  "name": "gssb-menu",
  "version": "0.0.1",
  "homepage": "https://github.com/evantill/gssb-menu",
  "authors": [
    "Eric Vantillard <eric.vantillard@evaxion.fr>"
  ],
  "description": "Gestion des Menus de la cantine pour l'Apel GSSB",
  "main": "?",
  "keywords": [
    "apel",
    "gssb",
    "menu",
    "cantine"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "node_modules",
    "bower_components",
    "out"
  ],
  "dependencies": {
    "jquery": "~2.0.3",
    "bootstrap": "~3.0.3",
    "modernizr": "~2.7.1",
    "moment": "~2.4.0",
    "respond": "~1.4.1",
    "console-polyfill": "~0.1.0",
    "bootstrap-datepicker": "https://github.com/eternicode/bootstrap-datepicker.git#master",
    "requirejs": "~2.1.9",
    "angular": "~1.2.6",
    "full-glyphicons": "https://github.com/evantill/full-glyphicons.git#feature-bower",
    "date.format": "~1.2.3"
  },
  "exportsOverride": {
    "full-glyphicons": {
      "css": "css/*",
      "img": "img/*"
    },
    "bootstrap": {
      "js-min": "dist/js/*.min.js",
      "js": "dist/js/*.js",
      "fonts": "dist/fonts/*",
      "css": "dist/css/*"
    },
    "modernizr": {
      "js": "modernizr.js"
    },
    "respond": {
      "js": "dest/*.js"
    },
    "*": {
      "js-min": "**/*.min.js",
      "js": "**/*.js",
      "css": "**/*.css"
    }
  }
}
