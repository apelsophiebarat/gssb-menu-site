        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="/vendor/jquery/js/jquery.min.js"><\/script>')</script>
        <script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
        <script src="/vendor/date.format/js/date.format.js"></script>
        <script src="/js/main.js"></script>
        {{{getBlock 'scripts' add=document.scripts}}}
        
        {{#if document.scriptOnReady}}
        <script type="text/javascript" defer='defer'>
            jQuery(function($) {
                $(document).ready(function() {
                    {{{document.scriptOnReady}}}
                });
            });
        </script>
        {{/if}}
        {{{partial 'google-analytics'}}}
    </body>
</html>
