    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{{getPreparedTitle}}</title>
        <meta name="description" content="{{site.description}}">
        <meta name="version" content="{{site.version}}">
        <meta name="viewport" content="width=device-width">
        {{{getBlock 'meta'}}}

        <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap-theme.min.css">
        <link rel="stylesheet" href="/vendor/full-glyphicons/css/glyphicons.css">
        <link rel="stylesheet" href="/css/main.css">
        {{{getBlock 'styles' add=document.styles}}}

        <script src="/vendor/console-polyfill/js/index.js"></script>
        <script src="/vendor/modernizr/js/modernizr.js"></script>
        <script src="/vendor/respond/js/respond.min.js"></script>

        <link rel="alternate" type="application/rss+xml" title="{{site.rss.menu.title}}" href="{{site.rss.menu.url}}"/>
    </head>
    <body>
        <!--[if lt IE 7]>
            <p class="chromeframe">{{{site.outdatedWarning}}}</p>
        <![endif]-->
        {{{partial 'navigation'}}}
        {{{content}}}
        {{{partial 'footer'}}}
